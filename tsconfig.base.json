{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "baseUrl": ".",
    "paths": {
      "@gioragutt/nx-blackbox": ["libs/nx-blackbox/src/index.ts"],
      "@gioragutt/nx-nest": ["libs/nx-nest/src/index.ts"],
      "@gioragutt/nx-openapi-client": ["libs/nx-openapi-client/src/index.ts"],
      "@gioragutt/nx-plugin-utils": ["libs/nx-plugin-utils/src/index.ts"],
      "@gioragutt/nx-workspace": ["libs/nx-workspace/src/index.ts"],
      "@nx-plugins/e2e-utils": ["libs/e2e-utils/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
